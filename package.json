{
  "name": "gulp-armadillo",
  "version": "0.0.0",
  "description": "Statis site generator put together with Gulp plugins",
  "main": "index.js",
  "files": [
    "index.js",
    "config",
    "lib",
    "tasks",
    "Readme.md",
    "LICENSE"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run ava",
    "ava": "NODE_ENV=test nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "reparo": "reparo -b master",
    "gulp": "gulp",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint --fix '**/*.js'",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Snugug/gulp-armadillo.git"
  },
  "keywords": [
    "gulp",
    "armadillo",
    "static-site",
    "markdown",
    "frontmatter",
    "jekyll",
    "rwd"
  ],
  "author": {
    "name": "Sam Richard",
    "email": "sam@snug.ug"
  },
  "engines": {
    "node": "^6"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Snugug/gulp-armadillo/issues"
  },
  "homepage": "https://github.com/Snugug/gulp-armadillo#readme",
  "dependencies": {
    "babel-preset-babili": "0.0.12",
    "browser-sync": "^2.18.5",
    "chalk": "^1.1.3",
    "config": "^1.24.0",
    "critical": "^0.8.4",
    "dateformat": "^2.0.0",
    "del": "^2.2.2",
    "eyeglass": "^1.2.1",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.1",
    "gulp-eslint": "^3.0.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-help": "^1.6.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-plumber": "^1.1.0",
    "gulp-replace": "^0.5.4",
    "gulp-rollup-stream": "^0.1.3",
    "gulp-sass": "^3.0.0",
    "gulp-sass-lint": "^1.3.2",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-util": "^3.0.7",
    "lazypipe": "^1.0.1",
    "lodash": "^4.17.2",
    "nunjucks": "^3.0.0",
    "prismjs": "^1.14.0",
    "pump": "^1.0.1",
    "remarkable": "^1.7.1",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-stream": "^1.24.1",
    "run-sequence": "^1.2.2",
    "stream-combiner": "^0.2.2",
    "sw-precache": "^5.0.0",
    "through2": "^2.0.3",
    "url": "^0.11.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "snyk": "^1.140.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "breakpoint-sass": "^2.7.1",
    "coveralls": "^2.11.15",
    "eslint": "^3.17.0",
    "eslint-config-punchcard": "^1.1.1",
    "eslint-config-xo-space": "^0.16.0",
    "eslint-plugin-ava": "^4.0.0",
    "ghooks": "^2.0.0",
    "leftpad": "0.0.0",
    "map-stream": "0.0.7",
    "nyc": "^10.0.0",
    "punchcard-commit-msg": "^1.1.0",
    "punchcard-semantic-release": "^2.0.4",
    "reparo": "^1.1.1-0",
    "semantic-release": "^6.3.2",
    "vinyl-fs": "^2.4.4",
    "vinyl-string": "^1.0.2"
  },
  "nyc": {
    "include": [
      "lib/**/*.js",
      "config/**/*.js"
    ],
    "all": true
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ]
  },
  "config": {
    "ghooks": {
      "commit-msg": "punchcard-commit-msg"
    }
  },
  "release": {
    "analyzeCommits": "punchcard-semantic-release/analyze",
    "generateNotes": "punchcard-semantic-release/notes"
  },
  "snyk": true
}
